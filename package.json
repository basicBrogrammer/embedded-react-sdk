{
  "name": "@gusto/embedded-react-sdk",
  "version": "0.0.5",
  "module": "./dist/index.js",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run openapi:generate && npm run i18n:generate && vite build",
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "commitlint": "commitlint --edit",
    "dev": "npm run build && vite build --watch",
    "doc": "typedoc",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "i18n:generate": "i18next-resources-for-ts interface -i ./src/i18n/en -o ./src/types/i18nresources.d.ts",
    "lint": "npm run lint:check -- --fix",
    "lint:check": "eslint .",
    "openapi": "npm run openapi:download && npm run openapi:generate",
    "openapi:download": "node build/getTargetSpecFromReadme.js",
    "openapi:generate": "node build/generateOpenApiFiles.js",
    "pack": "npm run build && npm pack",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "tsc": "tsc --pretty",
    "interface": "i18next-resources-for-ts interface -i ./src/i18n/en -o ./src/types/i18nresources.d.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@tanstack/react-query-devtools": "^5.59.15",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.13.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "i18next-resources-for-ts": "^1.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.6",
    "jest-transformer-svg": "^2.0.2",
    "jsdom": "^25.0.1",
    "msw": "^2.4.3",
    "openapi-typescript": "^7.4.1",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.80.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.10",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-sass-dts": "^1.3.29",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.9.0",
    "@internationalized/date": "^3.5.6",
    "@internationalized/number": "^3.5.4",
    "@tanstack/react-query": "^5.59.15",
    "classnames": "^2.5.1",
    "dompurify": "^3.1.7",
    "i18next": "^23.11.2",
    "openapi-fetch": "^0.12.2",
    "react-aria": "^3.35.1",
    "react-aria-components": "^1.4.1",
    "react-error-boundary": "^4.1.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.3",
    "react-robot": "^0.3.0",
    "robot3": "^0.4.1",
    "valibot": "^0.42.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
