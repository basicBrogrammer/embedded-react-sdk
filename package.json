{
  "name": "@gusto/embedded-react-sdk",
  "version": "0.13.2",
  "homepage": "https://github.com/Gusto/embedded-react-sdk",
  "bugs": {
    "url": "https://github.com/Gusto/embedded-react-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gusto/embedded-react-sdk.git"
  },
  "module": "./dist/index.js",
  "type": "module",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run i18n:generate && vite build",
    "build:ci": "npm run build && npm run lint:check && npm run format:check && npm run test:ci",
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "commitlint": "commitlint --edit",
    "adapter:docs:generate": "npx tsx ./build/generateAdapterPropDocs.ts",
    "theme:docs:generate": "npx tsx ./build/generateThemeDocs.ts",
    "watch:vite": "vite build --watch --mode development",
    "watch:translations": "node ./build/translationWatcher.js",
    "dev": "node ./build/prompt.js && npm run i18n:generate && npm-run-all --parallel watch:vite watch:translations ladle",
    "dev:setup": "npm link ../gws-flows/node_modules/react && (cd ../gws-flows && yarn link -r ../embedded-react-sdk)",
    "docs:events": "npx tsx ./build/eventTypeDocsEmitter.ts",
    "docs:sync": "npx tsx .docs/src/sync/syncManager.ts",
    "docs": "npx tsx .docs/src/preview/previewGenerator.ts",
    "docs:watch": "npx tsx .docs/src/preview/previewGenerator.ts --watch",
    "docs:publish": "npx tsx .docs/src/publish/readmePublisher.ts",
    "format": "prettier . --write --log-level error",
    "format:check": "prettier . --check --log-level error",
    "ladle": "ladle serve",
    "i18n:generate": "node ./build/interface.js",
    "lint": "npm run lint:check -- --fix",
    "lint:check": "eslint .",
    "pack": "npm run build && npm pack",
    "postversion": "git push",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "preversion": "npm test",
    "test": "vitest",
    "test:ci": "vitest --coverage",
    "tsc": "tsc --pretty"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.36.0",
    "@ladle/react": "^5.0.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.13",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-core": "^4.10.2",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest-axe": "^9.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "jsdom-testing-mocks": "^1.16.0",
    "lint-staged": "^16.2.0",
    "msw": "^2.11.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "sass-embedded": "^1.93.2",
    "ts-morph": "^27.0.0",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.44.0",
    "vite": "^6.2.7",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vite-plugin-stylelint": "^6.0.2",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@gusto/embedded-api": "^0.6.11",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@internationalized/date": "^3.9.0",
    "@internationalized/number": "^3.6.5",
    "classnames": "^2.5.1",
    "deepmerge": "^4.3.1",
    "dompurify": "^3.2.7",
    "i18next": "^25.5.2",
    "react-aria": "^3.43.1",
    "react-aria-components": "^1.12.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.3",
    "react-robot": "^1.2.0",
    "robot3": "^1.2.0",
    "zod": "^3.25.17"
  },
  "sideEffects": [
    "**/*.scss",
    "**/*.css",
    "src/contexts/ThemeProvider/ThemeProvider.tsx",
    "src/helpers/rem.ts"
  ]
}
