{
  "name": "@gusto/embedded-react-sdk",
  "version": "0.7.0",
  "homepage": "https://github.com/Gusto/embedded-react-sdk",
  "bugs": {
    "url": "https://github.com/Gusto/embedded-react-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gusto/embedded-react-sdk.git"
  },
  "module": "./dist/index.js",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run i18n:generate && vite build",
    "build:ci": "npm run build && npm run lint:check && npm run format:check && npm run test:ci",
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "commitlint": "commitlint --edit",
    "watch:vite": "vite build --watch",
    "watch:translations": "node ./build/translationWatcher.js",
    "dev": "node ./build/prompt.js && npm run i18n:generate && npm-run-all --parallel watch:vite watch:translations",
    "docs": "npx tsx ./build/eventTypeDocsEmitter.ts",
    "format": "prettier . --write --log-level error",
    "format:check": "prettier . --check --log-level error",
    "ladle": "ladle serve",
    "i18n:generate": "node ./build/interface.js",
    "lint": "npm run lint:check -- --fix",
    "lint:check": "eslint .",
    "pack": "npm run build && npm pack",
    "postversion": "git push",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "preversion": "npm test",
    "test": "vitest",
    "test:ci": "vitest --coverage",
    "tsc": "tsc --pretty"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.13.0",
    "@ladle/react": "^5.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.0.12",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.23.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "i18next-resources-for-ts": "^1.5.0",
    "jsdom": "^25.0.1",
    "jsdom-testing-mocks": "^1.13.1",
    "lint-staged": "^15.5.0",
    "msw": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.0",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.28.0",
    "vite": "^5.4.15",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-sass-dts": "^1.3.31",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@gusto/embedded-api": "^0.5.9",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.10.0",
    "@internationalized/date": "^3.7.0",
    "@internationalized/number": "^3.6.0",
    "classnames": "^2.5.1",
    "deepmerge": "^4.3.1",
    "dompurify": "^3.2.4",
    "i18next": "^23.16.8",
    "openapi-fetch": "^0.12.5",
    "react-aria": "^3.38.1",
    "react-aria-components": "^1.7.1",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-robot": "^1.2.0",
    "robot3": "^1.1.1",
    "valibot": "^0.42.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
